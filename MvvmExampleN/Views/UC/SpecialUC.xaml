<UserControl x:Class="MvvmExampleN.Views.UC.SpecialUC"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:MvvmExampleN.Views.UC"
             mc:Ignorable="d" 
             d:DesignHeight="100" d:DesignWidth="150" Width="150" Height="100" Margin="10">
    <Grid Background="Red" >
        <Button Command="{Binding SelectUCCommand}" FontSize="40" HorizontalAlignment="Center" VerticalAlignment="Center"
               Content="{Binding PrinterModel}"
               ></Button>
    </Grid>
</UserControl>
